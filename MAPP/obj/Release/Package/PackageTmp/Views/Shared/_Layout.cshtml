<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Map View App</title>
    @Styles.Render("~/Content/css")
    
    @Scripts.Render("~/bundles/modernizr")

</head>
<body  class="notransition no-hidden">
    <!-- Hero -->
    
    <div id="hero-container-map" style="margin-top: -50px;">
        <div id="home-map"></div>
        <div class="home-header map">
            <div class="home-logo osLight"><span class="fa fa-home"></span> Map View</div>
            <a href="#" class="home-navHandler visible-xs"><span class="fa fa-bars"></span></a>
            <div class="home-nav">
                <ul>

                    <li><a href="~/add.html" class="btn btn-green">Add New Member</a></li>
                </ul>
            </div>
        </div>

    </div>

    
    <div class="modal fade" id="contactAgent" role="dialog" aria-labelledby="contactLabel" aria-hidden="true">
        <div class="modal-dialog" style="margin-left:-25%">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="contactLabel">Member Detail</h4>
                </div>
                <div id="detail" class="modal-body">
                   
                </div>
                <div class="modal-footer">
                    <a href="#" data-dismiss="modal" class="btn btn-round btn-o btn-gray">Close</a>
                  
                </div>
            </div>
        </div>
    </div>
    @RenderBody()
    
   
    <script src="http://maps.googleapis.com/maps/api/js?sensor=true&amp;libraries=geometry&amp;libraries=places" type="text/javascript"></script>
    
    <script src="~/Scripts/employeeDetail.js"></script>
    @Scripts.Render("~/bundles/jquery")

 
    @RenderSection("scripts", required: false)
    <script>
       
        getEmployee();
        
       
    </script>
</body>
</html>
